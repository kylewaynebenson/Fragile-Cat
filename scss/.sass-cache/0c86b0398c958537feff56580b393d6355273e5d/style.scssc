//
// IMPORTING STYLES
//

// import normalize
@import "normalize";

@import "compass/utilities/general/clearfix";

// import grids
@import "grid";

// import mixins
@import "mixins";

@import "compass/css3";

/*
BASE (MOBILE) SIZE
This are the mobile styles. It's what people see on their phones. If
you set a great foundation, you won't need to add too many styles in
the other stylesheets. Remember, keep it light: Speed is Important.
*/

$blue: #3E94DB;
$dark-blue: #255B7F;
$darkest-blue: #153347;
$white: #fff;
$grey: #D3D3D3;
$dark-grey: #6B6B6B;
$darkest-grey: #262626;

$border-blue: 2px solid $blue;
$border-dark-blue: 2px solid $dark-blue;
$border-darkest-blue: 2px solid $darkest-blue;
$border-light: 2px solid $white;
$border-grey: 2px solid $grey;
$border-dark-grey: 2px solid $dark-grey;
$border-darkest-grey: 2px solid $darkest-grey;


$margin: 15px;
$double-margin: $margin * 2;
$half-margin: $margin / 2;

@mixin extend {
	padding: $double-margin;
	margin-right: -9999em;
	padding-right: 9999em;
	margin-left: -9999em;
	padding-left: 9999em;
}
@mixin shadow-blue {
	@include box-shadow($blue 2px 2px 0px);
}
@mixin shadow-dark-blue {
	@include box-shadow($dark-blue 2px 2px 0px);
}
@mixin shadow {
	@include box-shadow($darkest-blue 2px 2px 0px);
}
@mixin shadow-grey {
	@include box-shadow($grey 2px 2px 0px);
}
@mixin shadow-dark-grey {
	@include box-shadow($dark-grey 2px 2px 0px);
}
@mixin shadow-darkest-grey {
	@include box-shadow($darkest-grey 2px 2px 0px);
}
@mixin no-shadow {
	@include box-shadow(transparent 0px 0px 0px);
}
@mixin button {
	a {
		@include transition(background 0.125s ease-in);
		padding: 6px $margin 2px $margin;
		width: auto;
		margin-right: $margin;
		margin-bottom: $margin;
		text-decoration: none;
		@include inline-block;
		&:hover {
			margin-top: -2px;
			margin-left: -2px;
			margin-right: $margin +2px;
			margin-bottom: $margin +2px;
		}
		&:active {
			margin-top: 0px;
			margin-left: 0px;
			margin-right: $margin;
			margin-bottom: $margin;
			color: $grey;
		}
	}	
}
@mixin light-button {
	@include button;
	a {
		border: $border-grey;
		&:hover {
			@include shadow-grey;
			background: $white;
			color:  $grey;
		}
		&:active {
			background: $grey;
			@include no-shadow;
			color: $white;
		}
	}
}
@mixin grey-button {
	@include button;
	a {
		border: $border-dark-grey;
		&:hover {
			@include shadow-dark-grey;
			background: $white;
			color:  $grey;
		}
		&:active {
			background: $dark-grey;
			@include no-shadow;
		}
	}
}
@mixin greyest-button {
	@include button;
	a {
		border: $border-darkest-grey;
		&:hover {
			@include shadow-darkest-grey;
			background: $grey;
			color: $darkest-grey;
		}
		&:active {
			background: $darkest-grey;
			@include no-shadow;
			color: $dark-grey;
		}
	}
}
@mixin dark-blue-button {
	@include button;
	a {
		@include border-darkest-blue;
		color: $darkest-blue;
		background: $dark-blue;
		&:hover {
			color: $darkest-blue;
			@include shadow;
			background: $blue;
		}
		&:active {
			@include no-shadow;
			@include darkest-blue-background;
		}
	}
}
@mixin blue-button {
	@include button;
	a {
		@include border-dark-blue;
		color: $dark-blue;
		background: $blue;
		&:hover {
			color: $blue !important;
			@include shadow-dark-blue;
			background: $white;
		}
		&:active {
			@include no-shadow;
			@include dark-blue-background;
			color: $blue;
		}
	}
}
@mixin font-light {
	font-family: 'Nexa-Light', Nexa-Light, Gotham, 'Gotham', Gotham-Light, Helvetica;
	font-size: 40px;
	line-height: 100%;
}
@mixin font-book {
	font-family: 'Nexa-Book', Nexa-Book, Gotham, 'Gotham', Gotham-Book, Helvetica;
	font-size: 22px;
	line-height: 140%;	
}
@mixin font-bold {
	font-family: 'Nexa-Bold', Nexa-Bold, Gotham-Bold, 'Gotham-Bold', Gotham-Bold, Helvetica;
	font-size: 18px;
	line-height: 140%;
	em {
		font-family: 'Nexa-Bold-Italic', Nexa-Bold-Italic, Gotham-Bold, 'Gotham-Bold', Gotham-Bold-Italic, Helvetica;
	}
}
@mixin font-tall {
	font-family: 'Miso-Bold';
}
@mixin border-light {
	border: $border-light;
	@include border-radius(2px);
}
@mixin border-grey {
	border: $border-grey;
	@include border-radius(2px);
}
@mixin border-dark-grey {
	border: $border-dark-grey;
	@include border-radius(2px);
}
@mixin border-darkest-grey {
	border: $border-darkest-grey;
	@include border-radius(2px);
}
@mixin border-blue {
	border: $border-blue;
	@include border-radius(2px);
}
@mixin border-dark-blue {
	border: $border-dark-blue;
	@include border-radius(2px);
}
@mixin border-darkest-blue {
	border: $border-darkest-blue;
	@include border-radius(2px);
}
@mixin icon {
	font-family: 'mbiconsregular';
	font-weight: normal;
	font-size: 25px;
	line-height: 85%;
	a, a:hover, a:active {
		text-decoration: none;
	}
}
@mixin list-style {
	padding: $margin;
	margin-bottom: 0px !important;
	margin-top: 0px;
}
@mixin greyest-background {
	background: $dark-grey;
	color: $darkest-grey;
	.mini-directory {
		@include greyest-button;
	}
	a {
		color: $blue;
		text-decoration: none;
		border-bottom: 2px solid $blue;
		&:hover {
			color: $blue + #111;
		}
		&:active {
			color: $blue - #111;
		}
	}
	ul, ol {
		li {
			@include list-style;
			border: $border-darkest-grey;
			border-bottom: 0px;
			&:last-child {
				border-bottom: $border-darkest-grey;
			}
		}
		.fn-text {
			@include greyest-button;
		}
	}
	q {
		position: relative;
		@include border-darkest-grey;
		color: $darkest-grey;
		display: block;
		text-align: center;
		padding: $margin;
		padding-left: $margin + 4px;
		padding-right: $margin + 4px;
		text-indent: $margin;
		&:before {
			position: absolute;
			left: $margin;
			color: $darkest-grey;
			top: $double-margin + 5px;
			@include font-light;
			line-height: 0px;
			content:'\0022';
		}
		&:after {
			position: absolute;
			right: $margin;
			bottom: $margin;
			color: $darkest-grey;
			@include font-light;
			line-height: 0px;
			content:'\0022';
		}
	}
}
@mixin grey-background {
	background: $grey;
	color: $dark-grey;
	.mini-directory {
		@include grey-button;
	}
	a {
		color: $blue;
		text-decoration: none;
		border-bottom: 2px solid $blue;
		&:hover {
			color: $blue + #111;
		}
		&:active {
			color: $blue - #111;
		}
	}
	ul, ol {
		li {
			@include list-style;
			border: $border-dark-grey;
			border-bottom: 0px;
			&:last-child {
				border-bottom: $border-dark-grey;
			}
			span {
				display: inline;
				@include grey-button;
			}
		}
		.fn-text {
			@include grey-button;
		}
	}
	q {
		position: relative;
		@include border-dark-grey;
		display: block;
		text-align: center;
		color: $dark-grey;
		padding: $margin;
		padding-left: $margin + 4px;
		padding-right: $margin + 4px;
		text-indent: $margin;
		&:before {
			color: $dark-grey;
			position: absolute;
			left: $margin - 3px;
			top: $double-margin + 5px;
			@include font-light;
			line-height: 0px;
			content:'\0022';
		}
		&:after {
			color: $dark-grey;
			position: absolute;
			right: $margin - 3px;
			bottom: $margin;
			@include font-light;
			line-height: 0px;
			content:'\0022';
		}
	}

}
