				border-bottom: 1px solid #333;
			}
			.slposts-ulist-title {
				border-bottom: 0px;
				line-height: 130%;
				margin-bottom: $margin;
			}
			.slposts-ulist-excerpt {
					a {
						color: $white;
						text-decoration: none;
						margin-left: 5px;
						border-bottom: $border-light;
					}
			}
		}
	}
	.widget {

		ul {

			li {

				&:first-child {}

				&:last-child {}

				a {}

				/* deep nesting */
				ul {}

			}

		}

	}

